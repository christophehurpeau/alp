    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "pob-babel": "^17.2.0",
    "pob-release": "^3.1.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4"
  },
  "peerDependencies": {
    "alp-react-redux": "*",
    "prop-types": "^15.5.8",
    "react": "^15.5.4"
