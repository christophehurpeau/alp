{
  "name": "react-alp-subscribe-container",
  "version": "0.7.0",
  "description": "react subscribe-container, works with alp-websocket",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/react-alp-subscribe-container.git",
  "homepage": "https://github.com/alpjs/react-alp-subscribe-container",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "module": "./lib-module/index.js",
  "module-dev": "./lib-module-dev/index.js",
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "module:modern-browsers": "./lib-module-modern-browsers/index.js",
  "module:modern-browsers-dev": "./lib-module-modern-browsers-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "flow-runtime": "^0.16.0",
    "nightingale-logger": "^3.4.0"
  },
  "devDependencies": {
    "alp-react-redux": "^14.2.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest-node": "^0.4.0",
    "babel-preset-modern-browsers": "^10.0.0",
    "babel-preset-pob-react": "^0.2.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-pob": "^17.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^5.0.0",
    "pob-babel": "^18.1.5",
    "pob-release": "^3.1.0",
    "prettier": "^1.7.4",
    "prop-types": "^15.6.0",
    "react": "^16.1.1"
  },
  "peerDependencies": {
    "alp-react-redux": "*",
    "prop-types": "^15.5.8",
    "react": "^15.5.4 || ^16.0.0"
  }
}
