{
  "name": "alp-config",
  "version": "2.1.0",
  "description": "config in alp framework",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "alpjs/alp-config",
  "homepage": "https://github.com/alpjs/alp-config#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-config/issues"
  },
  "main": "./index.js",
  "webpack:main": "./dist/webpack/browser.js",
  "webpack:main-dev": "./dist/webpack-dev/browser.js",
  "webpack:main-modern-browsers": "./dist/webpack-modern-browsers/browser.js",
  "webpack:main-modern-browsers-dev": "./dist/webpack-modern-browsers-dev/browser.js",
  "scripts": {
    "build": "pob-build",
    "build:dev": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --fix -c .eslintrc.yml src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit || echo",
    "preversion": "npm run lint && npm run build && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch",
    "watch:dev": "pob-watch"
  },
  "dependencies": {
    "deep-freeze-es6": "^1.0.0",
    "minimist-argv": "^1.0.0",
    "parse-json-object-as-map": "^1.0.1",
    "stringify-json": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-modern-browsers": "^2.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-config-pob": "^6.0.1",
    "eslint-plugin-import": "^1.8.0",
    "pob-babel": "^0.1.2",
    "pob-release": "^2.0.2"
  }
}
