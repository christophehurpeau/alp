{
  "name": "alp-router",
  "version": "1.1.0",
  "description": "router for alp",
  "keywords": ["alp", "router"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/alp.git",
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-router/issues"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "browserslist": ["> 0.1%", "Firefox ESR", "not ie < 9", "not dead"],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "npm run typescript-check && eslint --ext .ts src/",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "peerDependencies": {
    "alp-types": "1.0.0-pre",
    "router-segments": "^1.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "babel-eslint": "10.0.1",
    "babel-preset-latest-node": "3.0.1",
    "babel-preset-modern-browsers": "13.0.1",
    "eslint": "5.12.1",
    "eslint-config-pob": "25.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-typescript": "0.14.0",
    "pob-babel": "23.5.5",
    "prettier": "1.16.0",
    "router-segments": "^1.0.0",
    "typescript": "3.3.1",
    "typescript-eslint-parser": "21.0.2"
  }
}
