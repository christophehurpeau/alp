{
  "name": "alp-hello-node-example",
  "private": true,
  "version": "11.0.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-hello-node-example"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "type": "module",
  "engines": {
    "node": ">=22.18.0"
  },
  "main": "./build/index-node.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "node": {
        "import": "./build/index-node.mjs"
      }
    }
  },
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs",
    "clean": "yarn clean:build",
    "clean:build": "pob-esbuild-clean-out build",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --quiet packages/alp-hello-node-example",
    "start": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "pob": {
    "bundler": "rollup-esbuild",
    "entries": [
      "index"
    ],
    "envs": [
      {
        "target": "node",
        "version": "22",
        "omitVersionInFileName": true
      }
    ],
    "typescript": true
  },
  "prettier": "@pob/root/prettier-config",
  "dependencies": {
    "alp-node": "8.0.0",
    "alp-node-auth": "11.0.0",
    "liwi-mongo": "12.0.0",
    "nightingale-app-console": "16.2.0",
    "nightingale-web-processor": "16.0.0",
    "router-segments": "11.0.0"
  },
  "devDependencies": {
    "@pob/rollup-esbuild": "6.7.0",
    "alp-rollup-plugin-config": "3.0.0",
    "typescript": "5.9.2"
  }
}
