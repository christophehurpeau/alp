{
  "name": "alp-react-redux",
  "version": "12.0.0",
  "description": "render with react and redux in alp framework",
  "keywords": [
    "alp",
    "auk"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-react-redux.git",
  "homepage": "https://github.com/alpjs/alp-react-redux#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-react-redux/issues"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "index.js",
  "module": "./lib-module/browser.js",
  "module-dev": "./lib-module-dev/browser.js",
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "module:modern-browsers": "./lib-module-modern-browsers/browser.js",
  "module:modern-browsers-dev": "./lib-module-modern-browsers-dev/browser.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "flow-runtime": "0.10.0",
    "modern-browsers": "^0.4.1",
    "nightingale-logger": "^3.4.0",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.6",
    "react-tree-walker": "^2.1.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest-node": "^0.4.0",
    "babel-preset-modern-browsers": "^10.0.0",
    "babel-preset-pob-react": "^0.2.4",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-pob": "^15.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.2.1",
    "husky": "^0.14.3",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.3",
    "pob-babel": "^18.0.4",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-beta.5",
    "react-dom": "^16.0.0-beta.5"
  },
  "peerDependencies": {
    "alp-dev": "^10.2.1",
    "prop-types": "^15.5.8",
    "react": "^15.6.1 || ^16.0.0-beta.5",
    "react-dom": "^15.6.1 || ^16.0.0-beta.5"
  }
}
