{
  "name": "alp-react-redux",
  "version": "5.0.0",
  "description": "render with react and redux in alp framework",
  "keywords": [
    "alp",
    "auk"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-react-redux.git",
  "homepage": "https://github.com/alpjs/alp-react-redux#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-react-redux/issues"
  },
  "main": "index.js",
  "webpack:main": "./lib-webpack/browser.js",
  "webpack:main-dev": "./lib-webpack-dev/browser.js",
  "webpack:main-modern-browsers": "./lib-webpack-modern-browsers/browser.js",
  "webpack:main-modern-browsers-dev": "./lib-webpack-modern-browsers-dev/browser.js",
  "scripts": {
    "build": "pob-build",
    "build:dev": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx --fix src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch",
    "watch:dev": "pob-watch"
  },
  "dependencies": {
    "fody": "^5.1.4",
    "fody-redux-app": "^5.0.0",
    "nightingale-logger": "^1.2.0",
    "react-pure-stateless-component": "0.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-modern-browsers": "^5.1.0",
    "babel-preset-modern-browsers-stage-1": "^1.0.0",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-config-pob": "^7.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "pob-babel": "^7.2.0",
    "pob-release": "^2.1.0",
    "react": "^15.3.0"
  },
  "peerDependencies": {
    "react": "^15.3.0"
  }
}
