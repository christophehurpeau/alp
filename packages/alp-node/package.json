{
  "name": "alp-node",
  "version": "0.32.1",
  "description": "framework based on koa 2",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "koa",
    "node5"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-node.git",
  "homepage": "https://github.com/alpjs/alp-node#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-node/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "alp-config": "^3.4.0",
    "alp-controller": "^0.1.0",
    "alp-errors-node": "^2.4.0",
    "alp-language": "^1.7.0",
    "alp-listen": "^1.3.1",
    "alp-params": "^0.3.0",
    "alp-translate": "^2.5.0",
    "findup-sync": "^1.0.0",
    "flow-runtime": "^0.10.0",
    "koa": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-static": "^4.0.1",
    "nightingale-logger": "^3.4.0",
    "node-fetch": "^1.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-latest-node": "^0.3.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-pob": "^14.1.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.2",
    "pob-babel": "^17.6.0",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3"
  }
}
