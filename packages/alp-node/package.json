{
  "name": "alp-node",
  "version": "0.38.0",
  "description": "framework based on koa 2",
  "keywords": ["springbokjs", "alp", "framework", "koa", "node5"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/alp.git",
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-node/issues"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "npm run typescript-check && eslint --ext .ts src/",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "alp-listen": "^1.5.0",
    "alp-node-config": "^3.5.1",
    "alp-node-errors": "^2.6.0",
    "alp-node-language": "^1.9.1",
    "alp-params": "^0.5.0",
    "alp-translate": "^2.7.0",
    "alp-types": "1.0.0-pre",
    "findup-sync": "^3.0.0",
    "koa": "^2.6.2",
    "koa-compress": "^3.0.0",
    "koa-static": "^5.0.0",
    "nightingale-logger": "^6.0.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@types/findup-sync": "^2.0.0",
    "@types/koa-compress": "^2.0.8",
    "@types/koa-static": "^4.0.0",
    "@types/minimist": "^1.2.0",
    "babel-eslint": "10.0.1",
    "babel-preset-latest-node": "3.0.1",
    "eslint": "5.12.1",
    "eslint-config-pob": "25.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-typescript": "0.14.0",
    "pob-babel": "23.5.5",
    "prettier": "1.16.0",
    "typescript": "3.3.1",
    "typescript-eslint-parser": "21.0.2"
  }
}
