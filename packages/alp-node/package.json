{
  "name": "alp-node",
  "version": "4.0.3",
  "description": "framework based on koa 2",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "koa",
    "node5"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-node"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-node/issues"
  },
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "main": "./dist/index-node14.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "AlpNodeApp": [
        "./dist/AlpNodeApp.d.ts"
      ],
      "fetch": [
        "./dist/fetch.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node14.mjs"
      }
    },
    "./AlpNodeApp": {
      "node": {
        "import": "./dist/AlpNodeApp-node14.mjs"
      }
    },
    "./fetch": {
      "node": {
        "import": "./dist/fetch-node14.mjs"
      }
    }
  },
  "module:node": "./dist/index-node14.mjs",
  "module:aliases-node": {
    "./AlpNodeApp.js": "./dist/AlpNodeApp-node14.es.js",
    "./fetch.js": "./dist/fetch-node14.es.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-node",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "AlpNodeApp",
      "fetch"
    ]
  },
  "dependencies": {
    "@types/koa": "^2.13.1",
    "@types/node-fetch": "^2.5.8",
    "alp-listen": "5.0.0",
    "alp-node-config": "7.0.0",
    "alp-node-errors": "6.0.0",
    "alp-node-language": "5.1.0",
    "alp-params": "4.1.0",
    "alp-translate": "6.0.1",
    "alp-types": "^3.0.0",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.0",
    "koa-static": "^5.0.0",
    "nightingale-logger": "^12.1.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@types/koa-compress": "4.0.3",
    "@types/koa-static": "4.0.2",
    "@types/minimist": "1.2.2",
    "pob-babel": "29.6.1",
    "typescript": "4.5.4"
  }
}
