{
  "name": "alp-node",
  "version": "1.5.0",
  "description": "framework based on koa 2",
  "keywords": ["springbokjs", "alp", "framework", "koa", "node5"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-node"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-node/issues"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["index"],
    "withReact": false
  },
  "dependencies": {
    "alp-listen": "^2.4.0",
    "alp-node-config": "^4.3.0",
    "alp-node-errors": "^3.4.0",
    "alp-node-language": "^2.3.0",
    "alp-params": "^1.3.0",
    "alp-translate": "^3.4.0",
    "alp-types": "^1.1.0",
    "findup-sync": "^4.0.0",
    "koa": "^2.7.0",
    "koa-compress": "^3.0.0",
    "koa-static": "^5.0.0",
    "nightingale-logger": "^6.2.2",
    "node-fetch": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@pob/eslint-config-babel-node": "32.1.0",
    "@pob/eslint-config-typescript": "32.1.0",
    "@types/findup-sync": "2.0.1",
    "@types/koa-compress": "2.0.9",
    "@types/koa-static": "4.0.1",
    "@types/minimist": "1.2.0",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "6.4.0",
    "pob-babel": "23.22.2",
    "prettier": "1.18.2"
  }
}
