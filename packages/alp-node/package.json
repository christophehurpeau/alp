{
  "name": "alp-node",
  "version": "3.0.0",
  "description": "framework based on koa 2",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "koa",
    "node5"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-node"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-node/issues"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      }
    },
    "./AlpNodeApp": {
      "node": {
        "development": {
          "import": "./dist/AlpNodeApp-node12-dev.mjs",
          "require": "./dist/AlpNodeApp-node12-dev.cjs.js"
        },
        "import": "./dist/AlpNodeApp-node12.mjs",
        "require": "./dist/AlpNodeApp-node12.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/alp-node",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "AlpNodeApp"
    ]
  },
  "dependencies": {
    "alp-listen": "^4.0.0",
    "alp-node-config": "^6.0.0",
    "alp-node-errors": "^5.0.0",
    "alp-node-language": "^4.0.0",
    "alp-params": "^3.0.0",
    "alp-translate": "^5.0.0",
    "alp-types": "^3.0.0",
    "findup-sync": "^4.0.0",
    "koa": "^2.11.0",
    "koa-compress": "^3.0.0",
    "koa-static": "^5.0.0",
    "nightingale-logger": "^11.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@types/findup-sync": "2.0.2",
    "@types/koa-compress": "2.0.9",
    "@types/koa-static": "4.0.1",
    "@types/minimist": "1.2.1",
    "babel-preset-latest-node": "5.2.0",
    "pob-babel": "26.3.0",
    "rollup": "2.36.2"
  }
}
