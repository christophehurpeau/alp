{
  "name": "alp-node",
  "version": "2.0.1",
  "description": "framework based on koa 2",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "koa",
    "node5"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-node"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-node/issues"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "eslint --ext .js,.ts --quiet *.js src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ],
    "withReact": false
  },
  "dependencies": {
    "alp-listen": "^3.0.1",
    "alp-node-config": "^5.0.1",
    "alp-node-errors": "^4.0.1",
    "alp-node-language": "^3.0.1",
    "alp-params": "^2.0.1",
    "alp-translate": "^4.0.1",
    "alp-types": "^2.0.0",
    "findup-sync": "^4.0.0",
    "koa": "^2.11.0",
    "koa-compress": "^3.0.0",
    "koa-static": "^5.0.0",
    "nightingale-logger": "^6.2.2",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@types/findup-sync": "2.0.2",
    "@types/koa-compress": "2.0.9",
    "@types/koa-static": "4.0.1",
    "@types/minimist": "1.2.0",
    "babel-preset-latest-node": "4.1.0",
    "pob-babel": "24.9.0"
  }
}
