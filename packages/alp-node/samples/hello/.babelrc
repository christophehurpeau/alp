{
  "stage": 1,
  "blacklist": [
    "es3.memberExpressionLiterals",
    "es3.propertyLiterals",
    "es5.properties.mutators",
  ],
  "optional": [
    "runtime",
    "es7.classProperties",
    "es7.decorators",
    "es7.exportExtensions",
    "optimisation.flow.forOf",
  ],
  "loose": [
    "es6.spread",
    "es6.destructuring",
    "es6.forOf"
  ],
  "plugins": ["add-jsdoc-properties"]
}
