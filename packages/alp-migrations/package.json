{
  "name": "alp-migrations",
  "version": "6.0.6",
  "description": "migrations scripts in alp",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-migrations"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "main": "./dist/index-node14.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node14.mjs"
      }
    }
  },
  "module:node": "./dist/index-node14.mjs",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "yarn clean:build",
    "clean:build": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-migrations",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "liwi-mongo": "^8.0.1 || ^9.0.0"
  },
  "dependencies": {
    "alp-types": "3.1.0",
    "nightingale-logger": "^12.1.4",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@types/node": "16.11.26",
    "@types/semver": "7.3.9",
    "liwi-mongo": "9.1.0",
    "pob-babel": "32.2.0",
    "typescript": "4.6.2"
  }
}
