{
  "name": "alp-migrations",
  "version": "9.0.0",
  "description": "migrations scripts in alp",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-migrations"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "sideEffects": false,
  "main": "./dist/index-node18.mjs",
  "types": "./dist/definitions/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/definitions/index.d.ts",
      "node": {
        "import": "./dist/index-node18.mjs"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.json",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-migrations",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "18"
      }
    ],
    "entries": [
      "index"
    ]
  },
  "prettier": "@pob/root/prettier-config",
  "peerDependencies": {
    "liwi-mongo": "^11.0.0",
    "router-segments": "^9.1.0"
  },
  "dependencies": {
    "alp-node": "7.0.0",
    "nightingale-logger": "^15.0.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@types/node": "20.11.24",
    "@types/semver": "7.5.6",
    "liwi-mongo": "11.0.0",
    "pob-babel": "38.0.2",
    "router-segments": "9.1.0",
    "typescript": "5.3.3"
  }
}
