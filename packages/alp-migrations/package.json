{
  "name": "alp-migrations",
  "version": "3.0.2",
  "description": "migrations scripts in alp",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-migrations"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.ts --quiet *.js src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "liwi-mongo": "^4.1.0 || ^5.0.0"
  },
  "dependencies": {
    "alp-types": "^2.0.0",
    "nightingale-logger": "^10.0.1",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@types/semver": "6.2.0",
    "babel-preset-latest-node": "4.1.0",
    "liwi-mongo": "5.1.0",
    "pob-babel": "24.12.0"
  }
}
