{
  "name": "alp-migrations",
  "version": "2.5.0",
  "description": "migrations scripts in alp",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-migrations"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["index"],
    "withReact": false
  },
  "peerDependencies": {
    "liwi-mongo": "^4.1.0"
  },
  "dependencies": {
    "alp-types": "^1.1.0",
    "nightingale-logger": "^6.2.2",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@pob/eslint-config-babel-node": "32.1.0",
    "@pob/eslint-config-typescript": "32.1.0",
    "@types/semver": "^6.0.0",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "6.4.0",
    "liwi-mongo": "4.2.1",
    "pob-babel": "23.22.2",
    "prettier": "1.18.2"
  }
}
