{
  "name": "alp-migrations",
  "version": "4.1.0",
  "description": "migrations scripts in alp",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-migrations"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/alp-migrations",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "liwi-mongo": "^7.0.0"
  },
  "dependencies": {
    "alp-types": "^2.0.0",
    "nightingale-logger": "^10.0.1",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@types/node": ">=12.0.0",
    "@types/semver": "7.3.4",
    "babel-preset-latest-node": "5.2.0",
    "liwi-mongo": "7.4.2",
    "pob-babel": "26.3.0",
    "rollup": "2.36.1"
  }
}
