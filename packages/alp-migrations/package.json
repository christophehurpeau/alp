{
  "name": "alp-migrations",
  "version": "0.2.0",
  "description": "migrations scripts in alp",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-migrations.git",
  "homepage": "https://github.com/alpjs/alp-migrations",
  "main": "./index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx --fix src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "dependencies": {
    "nightingale-logger": "^1.2.0",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-config-pob": "^7.2.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "liwi": "^0.5.0",
    "pob-babel": "^7.3.0",
    "pob-release": "^2.1.1"
  },
  "peerDependencies": {
    "liwi": "^0.5.0"
  }
}
