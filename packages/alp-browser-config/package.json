{
  "name": "alp-browser-config",
  "version": "5.1.2",
  "description": "config in alp framework",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-browser-config"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-browser-config/issues"
  },
  "browserslist": [
    "> 0.2%",
    "Firefox ESR",
    "not ie < 12",
    "not dead",
    "not op_mini all"
  ],
  "main": "./dist/index-browser.es.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "exports": {
    ".": {
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.mjs"
          },
          "import": "./dist/index-browsermodern.mjs"
        },
        "development": {
          "import": "./dist/index-browser-dev.mjs",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.mjs",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/alp-browser-config",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "alp-types": "^2.0.0",
    "deep-freeze-es6": "^1.3.1",
    "parse-json-object-as-map": "^1.2.0",
    "stringify-json": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "26.3.0",
    "rollup": "2.36.2"
  }
}
