{
  "name": "alp-node-auth",
  "version": "2.10.1",
  "description": "authentication with alp",
  "keywords": ["alp"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-node-auth"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "strategies/*": ["ts3.1/strategies.d.ts"]
    }
  },
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["index"],
    "withReact": false
  },
  "peerDependencies": {
    "alp-node": "^1.1.3",
    "liwi-mongo": "^4.1.0 || ^5.0.0",
    "liwi-store": "^4.1.0"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "@types/simple-oauth2": "^2.2.1",
    "alp-types": "^1.1.0",
    "cookies": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "nightingale-logger": "^6.2.2",
    "simple-oauth2": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@pob/eslint-config-babel-node": "32.1.0",
    "@pob/eslint-config-typescript": "32.1.0",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "6.4.0",
    "liwi-mongo": "5.0.4",
    "liwi-store": "4.3.2",
    "pob-babel": "23.22.2",
    "prettier": "1.18.2"
  }
}
