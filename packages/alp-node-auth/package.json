{
  "name": "alp-node-auth",
  "version": "5.3.1",
  "description": "authentication with alp",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-node-auth"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "main": "./dist/index-node14.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "strategies/*": [
        "strategies/strategies.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node14.mjs"
      }
    },
    "./strategies/dropbox": {
      "import": "./strategies/dropbox.mjs",
      "require": "./strategies/dropbox.js"
    },
    "./strategies/facebook": {
      "import": "./strategies/facebook.mjs",
      "require": "./strategies/facebook.js"
    },
    "./strategies/foursquare": {
      "import": "./strategies/foursquare.mjs",
      "require": "./strategies/foursquare.js"
    },
    "./strategies/github": {
      "import": "./strategies/github.mjs",
      "require": "./strategies/github.js"
    },
    "./strategies/google": {
      "import": "./strategies/google.mjs",
      "require": "./strategies/google.js"
    },
    "./strategies/slack": {
      "import": "./strategies/slack.mjs",
      "require": "./strategies/slack.js"
    }
  },
  "module:node": "./dist/index-node14.mjs",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-node-auth",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ],
    "extraEntries": [
      "strategies/dropbox",
      "strategies/facebook",
      "strategies/foursquare",
      "strategies/github",
      "strategies/google",
      "strategies/slack"
    ]
  },
  "peerDependencies": {
    "alp-node": "^3.0.1",
    "alp-router": "^4.0.1",
    "liwi-mongo": "^8.0.1"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.1",
    "@types/simple-oauth2": "^2.5.1",
    "alp-types": "^3.0.0",
    "cookies": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "nightingale-logger": "^12.1.2",
    "simple-oauth2": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "alp-node": "^3.2.2",
    "alp-router": "^4.1.3",
    "liwi-mongo": "8.0.4",
    "pob-babel": "29.6.1",
    "typescript": "4.5.4"
  }
}
