  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "@types/simple-oauth2": "^1.1.1",
    "alp-types": "^1.0.0",
    "cookies": "^0.7.3",
    "jsonwebtoken": "^8.5.0",
    "nightingale-logger": "^6.0.0",
    "simple-oauth2": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@typescript-eslint/eslint-plugin": "1.4.2",
