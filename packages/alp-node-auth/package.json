{
  "name": "alp-node-auth",
  "version": "4.0.0",
  "description": "authentication with alp",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-node-auth"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "engines": {
    "node": ">=10.13.0"
  },
  "sideEffects": false,
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "strategies/*": [
        "ts3.1/strategies.d.ts"
      ]
    }
  },
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn --cwd ../.. lint packages/alp-node-auth",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "alp-node": "^2.1.1",
    "liwi-mongo": "^7.0.0"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.5",
    "@types/simple-oauth2": "^2.5.1",
    "alp-types": "^2.0.0",
    "cookies": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "nightingale-logger": "^10.0.1",
    "simple-oauth2": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "babel-preset-latest-node": "4.1.0",
    "liwi-mongo": "7.0.0",
    "pob-babel": "24.12.0"
  }
}
