{
  "name": "alp-node-auth",
  "version": "7.2.0",
  "description": "authentication with alp",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-node-auth"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./dist/index-node16.mjs",
  "types": "./dist/definitions/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "strategies/*": [
        "strategies/strategies.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node16.mjs"
      }
    },
    "./strategies/dropbox": "./strategies/dropbox.js",
    "./strategies/facebook": "./strategies/facebook.js",
    "./strategies/foursquare": "./strategies/foursquare.js",
    "./strategies/github": "./strategies/github.js",
    "./strategies/google": "./strategies/google.js",
    "./strategies/slack": "./strategies/slack.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.json",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-node-auth",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "16"
      }
    ],
    "entries": [
      "index"
    ],
    "extraEntries": [
      "strategies/dropbox",
      "strategies/facebook",
      "strategies/foursquare",
      "strategies/github",
      "strategies/google",
      "strategies/slack"
    ]
  },
  "peerDependencies": {
    "alp-node": "^5.0.0",
    "alp-router": "^6.0.0",
    "liwi-mongo": "^10.2.0",
    "router-segments": "^6.0.0"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "@types/simple-oauth2": "^4.0.0",
    "alp-types": "3.1.0",
    "cookies": "^0.8.0",
    "jsonwebtoken": "^9.0.0",
    "nightingale-logger": "^13.0.0",
    "simple-oauth2": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "alp-node": "5.1.2",
    "alp-router": "6.1.2",
    "liwi-mongo": "10.2.0",
    "pob-babel": "35.5.0",
    "router-segments": "6.0.0",
    "typescript": "4.9.5"
  }
}
