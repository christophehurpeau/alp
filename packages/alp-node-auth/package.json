    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "peerDependencies": {
    "liwi-mongo": "^4.1.0",
    "liwi-store": "^4.1.0"
