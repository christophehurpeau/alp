{
  "name": "alp-translate",
  "version": "4.2.1",
  "description": "Translation in alp framework",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-translate"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-translate/issues"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "browserslist": [
    "> 0.2%",
    "Firefox ESR",
    "not ie < 12",
    "not dead",
    "not op_mini all"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "browser": [
        "./dist/browser.d.ts"
      ]
    }
  },
  "module": "./dist/index-browser.es.js",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      },
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.mjs"
          },
          "import": "./dist/index-browsermodern.mjs"
        },
        "development": {
          "import": "./dist/index-browser-dev.mjs",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.mjs",
        "require": "./dist/index-browser.cjs.js"
      }
    },
    "./browser": {
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/browser-browsermodern-dev.mjs"
          },
          "import": "./dist/browser-browsermodern.mjs"
        },
        "development": {
          "import": "./dist/browser-browser-dev.mjs",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/browser-browser.mjs",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/alp-translate",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "browser"
    ]
  },
  "dependencies": {
    "alp-types": "^2.0.0",
    "intl-messageformat": "^9.3.9",
    "nightingale-logger": "^10.0.1",
    "react-alp-context": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "babel-preset-latest-node": "5.2.0",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "26.3.0",
    "rollup": "2.36.2"
  }
}
