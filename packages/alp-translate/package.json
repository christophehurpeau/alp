{
  "name": "alp-translate",
  "version": "5.1.3",
  "description": "Translation in alp framework",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-translate"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-translate/issues"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "browserslist": [
    "> 0.2%",
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "not ie < 12",
    "not ios_saf < 10",
    "not dead",
    "not op_mini all"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "browser": [
        "./dist/browser.d.ts"
      ]
    }
  },
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      },
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.es.js"
          },
          "import": "./dist/index-browsermodern.es.js"
        },
        "development": {
          "import": "./dist/index-browser-dev.es.js",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.es.js",
        "require": "./dist/index-browser.cjs.js"
      }
    },
    "./browser": {
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.es.js"
          },
          "import": "./dist/index-browsermodern.es.js"
        },
        "development": {
          "import": "./dist/index-browser-dev.es.js",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.es.js",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "module:node": "./dist/index-node12.mjs",
  "module:node-dev": "./dist/index-node12-dev.mjs",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "module:aliases-browser": {
    "./browser.js": "./dist/index-browser.es.js"
  },
  "module:aliases-browser-dev": {
    "./browser.js": "./dist/index-browser-dev.es.js"
  },
  "module:aliases-modern-browsers": {
    "./browser.js": "./dist/index-browsermodern.es.js"
  },
  "module:aliases-modern-browsers-dev": {
    "./browser.js": "./dist/index-browsermodern-dev.es.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. run eslint --ext .js,.mjs,.ts --report-unused-disable-directives --quiet packages/alp-translate",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "browser"
    ]
  },
  "dependencies": {
    "alp-types": "^3.0.0",
    "intl-messageformat": "^9.3.9",
    "nightingale-logger": "^11.0.5",
    "react-alp-context": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/preset-env": "7.13.12",
    "babel-preset-latest-node": "5.5.1",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "26.8.0",
    "rollup": "2.43.1"
  }
}
