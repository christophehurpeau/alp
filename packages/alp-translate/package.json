{
  "name": "alp-translate",
  "version": "7.3.0",
  "description": "Translation in alp framework",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-translate"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-translate/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      "defaults",
      "> 0.2%",
      "not ie < 12",
      "not safari < 10",
      "not ios_saf < 10"
    ]
  },
  "main": "./dist/index-node16.mjs",
  "types": "./dist/definitions/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "browser": [
        "./dist/browser.d.ts"
      ]
    }
  },
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node16.mjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js"
      }
    },
    "./browser": {
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.json",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-translate",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "16"
      },
      {
        "target": "browser",
        "version": "modern"
      },
      {
        "target": "browser"
      }
    ],
    "entries": [
      "index",
      "browser"
    ]
  },
  "dependencies": {
    "alp-types": "3.1.0",
    "intl-messageformat": "^10.0.0",
    "nightingale-logger": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "pob-babel": "35.6.2",
    "typescript": "4.9.5"
  }
}
