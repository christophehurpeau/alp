{
  "name": "alp-listen",
  "version": "2.3.1",
  "description": "Listen function, using config and logger for alp server",
  "keywords": ["alp"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-listen"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-listen/issues"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["index"],
    "withReact": false
  },
  "dependencies": {
    "alp-node-config": "^4.2.2",
    "nightingale-logger": "^6.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@pob/eslint-config-babel-node": "32.0.13",
    "@pob/eslint-config-typescript": "32.0.13",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "6.3.0",
    "pob-babel": "23.20.0",
    "prettier": "1.18.2"
  }
}
