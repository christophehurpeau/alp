{
  "name": "alp-listen",
  "version": "1.4.0",
  "description": "Listen function, using config and logger for alp server",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-listen.git",
  "homepage": "https://github.com/alpjs/alp-listen#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-listen/issues"
  },
  "main": "./index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "flow-runtime": "0.10.0",
    "nightingale-logger": "^3.0.0-beta.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-latest-node": "^0.4.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-pob": "^15.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.2",
    "pob-babel": "^18.1.0",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3"
  },
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "engines": {
    "node": ">=6.5.0"
  }
}
