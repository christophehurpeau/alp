{
  "name": "alp-react",
  "version": "19.0.2",
  "description": "render with react in alp framework",
  "keywords": [
    "alp",
    "auk"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-react"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-react/issues"
  },
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "main": "./dist/index-node14.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "browser": [
        "./dist/browser.d.ts"
      ]
    }
  },
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node14.mjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js"
      }
    },
    "./browser": {
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js"
      }
    }
  },
  "module:node": "./dist/index-node14.mjs",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:aliases-browser": {
    "./browser.js": "./dist/index-browser.es.js"
  },
  "module:aliases-modern-browsers": {
    "./browser.js": "./dist/index-browsermodern.es.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "yarn clean:build",
    "clean:build": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-react",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "browser"
    ],
    "jsx": true
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "alp-types": "3.1.0",
    "content-loaded": "^1.2.1",
    "modern-browsers": "^2.0.0",
    "nightingale-logger": "^12.1.4",
    "react-alp-context": "4.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "32.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.5.5"
  }
}
