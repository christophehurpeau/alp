{
  "name": "alp-react",
  "version": "17.0.1",
  "description": "render with react in alp framework",
  "keywords": [
    "alp",
    "auk"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-react"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-react/issues"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "> 0.1%",
    "Firefox ESR",
    "not ie < 9",
    "not dead"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "browser": [
        "./dist/browser.d.ts"
      ]
    }
  },
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "module:aliases-browser": {
    "./browser.js": "./dist/index-browser.es.js"
  },
  "module:aliases-browser-dev": {
    "./browser.js": "./dist/index-browser-dev.es.js"
  },
  "module:aliases-modern-browsers": {
    "./browser.js": "./dist/index-browsermodern.es.js"
  },
  "module:aliases-modern-browsers-dev": {
    "./browser.js": "./dist/index-browsermodern-dev.es.js"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn --cwd ../.. lint packages/alp-react",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "browser"
    ],
    "jsx": true
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^6.1.0",
    "alp-types": "^2.0.0",
    "content-loaded": "^1.2.1",
    "modern-browsers": "^1.3.0",
    "nightingale-logger": "^10.0.1",
    "react-alp-context": "^2.1.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "babel-preset-latest-node": "4.1.0",
    "babel-preset-modern-browsers": "14.2.1",
    "pob-babel": "24.12.0",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  }
}
