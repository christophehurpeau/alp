{
  "name": "alp-react",
  "version": "15.8.0",
  "description": "render with react in alp framework",
  "keywords": ["alp", "auk"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-react"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-react/issues"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "browserslist": ["> 0.1%", "Firefox ESR", "not ie < 9", "not dead"],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "browser": ["./dist/browser.d.ts"]
    }
  },
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "module:aliases-node": {
    "./hot.js": "./dist/hot-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./hot.js": "./dist/hot-node10-dev.es.js"
  },
  "module:aliases-browser": {
    "./browser.js": "./dist/index-browser.es.js",
    "./hot.js": "./dist/hot-browser.es.js"
  },
  "module:aliases-browser-dev": {
    "./browser.js": "./dist/index-browser-dev.es.js",
    "./hot.js": "./dist/hot-browser-dev.es.js"
  },
  "module:aliases-modern-browsers": {
    "./browser.js": "./dist/index-browsermodern.es.js",
    "./hot.js": "./dist/hot-browsermodern.es.js"
  },
  "module:aliases-modern-browsers-dev": {
    "./browser.js": "./dist/index-browsermodern-dev.es.js",
    "./hot.js": "./dist/hot-browsermodern-dev.es.js"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "eslint --ext .ts,.tsx --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": ["es"]
      },
      {
        "target": "browser",
        "formats": ["cjs", "es"]
      }
    ],
    "entries": ["index", "browser", "hot"],
    "withReact": true
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devPeerDependencies": {
    "alp-dev": "*"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.8",
    "alp-types": "^1.1.0",
    "content-loaded": "^1.2.1",
    "modern-browsers": "^1.3.0",
    "nightingale-logger": "^6.2.2",
    "react-alp-context": "^1.3.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@pob/eslint-config-typescript": "32.1.0",
    "@pob/eslint-config-typescript-react": "32.1.0",
    "alp-dev": "^12.7.0",
    "babel-preset-latest-node": "3.2.1",
    "babel-preset-modern-browsers": "14.0.0",
    "eslint": "6.4.0",
    "pob-babel": "23.22.2",
    "prettier": "1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  }
}
