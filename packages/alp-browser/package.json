    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@types/webpack-env": "^1.13.9",
    "alp-browser-config": "^4.1.1",
