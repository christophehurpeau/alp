{
  "name": "alp-browser",
  "version": "0.21.0",
  "description": "framework for browser based on ibex",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "ibex"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-browser.git",
  "homepage": "https://github.com/alpjs/alp-browser#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-browser/issues"
  },
  "main": "./index.js",
  "webpack:main": "./lib-webpack/index.js",
  "webpack:main-dev": "./lib-webpack-dev/index.js",
  "webpack:main-modern-browsers": "./lib-webpack-modern-browsers/index.js",
  "webpack:main-modern-browsers-dev": "./lib-webpack-modern-browsers-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx --fix src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "dependencies": {
    "alauda": "^2.3.0",
    "alp-config": "^3.0.0",
    "alp-controller": "^0.1.0",
    "alp-errors-browser": "^0.3.0",
    "alp-language": "^1.4.0",
    "alp-params": "^0.3.0",
    "alp-translate": "^2.3.1",
    "content-loaded": "^1.1.0",
    "ibex": "^4.2.0",
    "nightingale-logger": "^3.0.0-beta.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-modern-browsers": "^6.0.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-config-pob": "^8.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "pob-babel": "^12.5.5",
    "pob-release": "^2.2.2",
    "tcomb-forked": "^3.4.0"
  }
}
