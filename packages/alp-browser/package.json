{
  "name": "alp-browser",
  "version": "0.23.2",
  "description": "framework for browser based on ibex",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "ibex"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-browser.git",
  "homepage": "https://github.com/alpjs/alp-browser#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-browser/issues"
  },
  "main": "./index.js",
  "module": "./lib-module/index.js",
  "module-dev": "./lib-module-dev/index.js",
  "module:modern-browsers": "./lib-module-modern-browsers/index.js",
  "module:modern-browsers-dev": "./lib-module-modern-browsers-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "alp-config": "^3.5.0",
    "alp-language": "^1.8.0",
    "alp-translate": "^2.6.0",
    "flow-runtime": "0.10.0",
    "ibex": "^4.5.0",
    "nightingale-logger": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-modern-browsers": "^10.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-pob": "^16.0.4",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.4",
    "pob-babel": "^18.1.5",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3"
  }
}
