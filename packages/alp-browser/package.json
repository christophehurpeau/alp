{
  "name": "alp-browser",
  "version": "2.2.1",
  "description": "framework for browser based on ibex",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "ibex"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-browser"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-browser/issues"
  },
  "browserslist": [
    "> 0.1%",
    "Firefox ESR",
    "not ie < 9",
    "not dead"
  ],
  "main": "./dist/index-browser.es.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn --cwd ../.. lint packages/alp-browser",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@types/webpack-env": "^1.14.1",
    "alp-browser-config": "^5.1.1",
    "alp-browser-language": "^3.1.1",
    "alp-translate": "^4.2.0",
    "alp-types": "^2.0.0",
    "ibex": "^6.1.1",
    "nightingale-logger": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "babel-preset-modern-browsers": "14.2.1",
    "pob-babel": "24.12.0"
  }
}
