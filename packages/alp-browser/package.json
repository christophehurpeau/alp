{
  "name": "alp-browser",
  "version": "0.4.0",
  "description": "framework for browser based on ibex",
  "keywords": [
    "springbokjs",
    "alp",
    "framework",
    "ibex"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "alpjs/alp-browser",
  "homepage": "https://github.com/alpjs/alp-browser#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-browser/issues"
  },
  "webpack:main": "./dist/webpack/index.js",
  "webpack:main-dev": "./dist/webpack-dev/index.js",
  "webpack:main-modern-browsers": "./dist/webpack-modern-browsers/index.js",
  "webpack:main-modern-browsers-dev": "./dist/webpack-modern-browsers-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "build:dev": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --fix -c .eslintrc.yml src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit || echo",
    "preversion": "npm run lint && npm run build && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch",
    "watch:dev": "pob-watch"
  },
  "dependencies": {
    "alp-config": "^2.1.1",
    "alp-language": "^1.1.1",
    "alp-limosa": "^2.1.2",
    "alp-translate": "^0.1.1",
    "ibex": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-modern-browsers": "^2.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-config-pob": "^6.0.1",
    "eslint-plugin-import": "^1.8.0",
    "pob-babel": "^0.1.2",
    "pob-release": "^2.0.2"
  }
}
