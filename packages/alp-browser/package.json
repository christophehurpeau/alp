    "alp-translate": "^3.1.1",
    "alp-types": "^1.0.0",
    "ibex": "^5.1.1",
    "nightingale-logger": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@pob/eslint-config-babel": "29.2.0",
    "@pob/eslint-config-typescript": "29.2.0",
    "babel-preset-modern-browsers": "14.0.0",
    "eslint": "5.16.0",
    "pob-babel": "23.12.0",
    "prettier": "1.16.4",
    "typescript": "3.4.2"
