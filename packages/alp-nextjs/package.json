{
  "name": "alp-nextjs",
  "version": "1.2.1",
  "description": "configs and utils for nextjs",
  "keywords": [
    "springbokjs",
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-nextjs"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-nextjs/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "main": "./dist/index-node16.mjs",
  "types": "./dist/definitions/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node16.mjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js"
      }
    },
    "./preset-babel.cjs": "./preset-babel.cjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.json",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-nextjs",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {
    "jsx": true,
    "babelEnvs": [
      {
        "target": "node",
        "version": "16"
      },
      {
        "target": "browser",
        "version": "modern"
      },
      {
        "target": "browser"
      }
    ],
    "entries": [
      "index"
    ],
    "extraEntries": [
      "preset-babel.cjs"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.11.6",
    "next": "^12.2.5",
    "react": "^18.0.0",
    "react-native-web": "^0.18.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "babel-preset-pob-env": "9.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "babel-preset-modern-browsers": "15.0.2",
    "next": "12.2.5",
    "pob-babel": "35.3.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native-web": "0.18.9",
    "typescript": "4.9.3"
  }
}
