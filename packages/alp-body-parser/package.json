{
  "name": "alp-body-parser",
  "version": "3.2.2",
  "description": "body parser in alp framework",
  "keywords": ["alp", "body-parser"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-body-parser"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-body-parser/issues"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["index"],
    "withReact": false
  },
  "peerDependencies": {
    "alp-node": "^1.0.0"
  },
  "dependencies": {
    "@types/co-body": "^0.0.3",
    "@types/koa": "^2.0.48",
    "co-body": "^6.0.0",
    "koa": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@pob/eslint-config-babel-node": "32.1.0",
    "@pob/eslint-config-typescript": "32.1.0",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "6.3.0",
    "pob-babel": "23.22.1",
    "prettier": "1.18.2"
  }
}
