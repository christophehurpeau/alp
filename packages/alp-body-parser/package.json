{
  "name": "alp-body-parser",
  "version": "4.2.0",
  "description": "body parser in alp framework",
  "keywords": [
    "alp",
    "body-parser"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-body-parser"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-body-parser/issues"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "yarn --cwd ../.. lint packages/alp-body-parser",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "alp-node": "^1.0.0"
  },
  "dependencies": {
    "@types/co-body": "^0.0.4",
    "@types/koa": "^2.11.0",
    "co-body": "^6.0.0",
    "koa": "^2.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "babel-preset-latest-node": "4.1.0",
    "pob-babel": "24.12.0"
  }
}
