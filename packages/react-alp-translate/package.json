{
  "name": "react-alp-translate",
  "version": "4.1.3",
  "description": "react translate",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/react-alp-translate"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "engines": {
    "node": ">=12.10.0"
  },
  "browserslist": [
    "> 0.2%",
    "Firefox ESR",
    "not ie < 12",
    "not dead",
    "not op_mini all"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      },
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.mjs"
          },
          "import": "./dist/index-browsermodern.mjs"
        },
        "development": {
          "import": "./dist/index-browser-dev.mjs",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.mjs",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.ts,.tsx --report-unused-disable-directives --quiet packages/react-alp-translate",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ],
    "jsx": true
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "alp-types": "^2.0.0",
    "react-alp-context": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "babel-preset-latest-node": "5.2.0",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "26.3.0",
    "react": "16.14.0",
    "rollup": "2.36.2"
  }
}
