{
  "name": "react-alp-translate",
  "version": "4.0.1",
  "description": "react translate",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/react-alp-translate"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": ["> 0.1%", "Firefox ESR", "not ie < 9", "not dead"],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.ts,.tsx --quiet src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": ["es"]
      },
      {
        "target": "browser",
        "formats": ["cjs", "es"]
      }
    ],
    "entries": ["index"],
    "withReact": true
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "alp-types": "^2.0.0",
    "react-alp-context": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@pob/eslint-config-typescript": "33.0.8",
    "@pob/eslint-config-typescript-react": "33.0.8",
    "babel-preset-latest-node": "4.0.1",
    "babel-preset-modern-browsers": "14.1.1",
    "eslint": "6.7.2",
    "pob-babel": "24.1.0",
    "prettier": "1.19.1",
    "react": "16.11.0"
  }
}
