{
  "name": "alp-auth",
  "version": "0.14.1",
  "description": "authentication with alp",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-auth.git",
  "homepage": "https://github.com/alpjs/alp-auth",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./index.js",
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:api": "rm -Rf docs/ ; mkdir docs/ ; pob-build doc ; jsdoc README.md lib-doc --recurse --destination docs/ --configure jsdoc.conf.json ; rm -Rf lib-doc",
    "generate:docs": "npm run generate:api",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "cookies": "^0.7.1",
    "flow-runtime": "0.16.0",
    "jsonwebtoken": "^8.1.0",
    "nightingale-logger": "^3.4.0",
    "promise-callback-factory": "^1.4.0",
    "simple-oauth2": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-latest-node": "^0.4.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-pob": "^17.0.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^5.0.0",
    "liwi": "^0.18.4",
    "minami": "^1.2.3",
    "pob-babel": "^18.1.3",
    "pob-release": "^3.1.0",
    "prettier": "^1.8.2"
  },
  "peerDependencies": {
    "liwi": ">=0.8.0 <1.0.0"
  }
}
