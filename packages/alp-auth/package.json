  "main": "./index.js",
  "webpack:main": "./lib-webpack/index.browser.js",
  "webpack:main-dev": "./lib-webpack-dev/index.browser.js",
  "webpack:main-modern-browsers": "./lib-webpack-modern-browsers/index.browser.js",
