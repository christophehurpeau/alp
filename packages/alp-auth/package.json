{
  "name": "alp-auth",
  "version": "0.5.0",
  "description": "authentication with alp",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-auth.git",
  "homepage": "https://github.com/alpjs/alp-auth",
  "main": "./index.js",
  "webpack:main": "./lib-webpack/browser.js",
  "webpack:main-dev": "./lib-webpack-dev/browser.js",
  "webpack:main-modern-browsers": "./lib-webpack-modern-browsers/browser.js",
  "webpack:main-modern-browsers-dev": "./lib-webpack-modern-browsers-dev/browser.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:api": "rm -Rf docs/ ; mkdir docs/ ; pob-build doc ; jsdoc README.md lib-doc --recurse --destination docs/ --configure jsdoc.conf.json ; rm -Rf lib-doc",
    "generate:docs": "npm run generate:api",
    "lint": "eslint --ext .js,.jsx --fix src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true ; ln -s ../../git-hooks/prepare-commit-msg .git/hooks/prepare-commit-msg 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "dependencies": {
    "cookies": "^0.6.1",
    "jsonwebtoken": "^7.1.9",
    "nightingale-logger": "^1.3.0",
    "promise-callback-factory": "^1.0.0",
    "simple-oauth2": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-jsdoc-annotations": "^5.1.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-jsdoc": "^0.4.0",
    "babel-preset-modern-browsers": "^5.1.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-config-pob": "^7.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "jsdoc": "^3.4.1",
    "komet": "^0.1.3",
    "komet-karma": "^0.1.2",
    "liwi": "^0.7.4",
    "minami": "^1.1.1",
    "pob-babel": "^8.3.2",
    "pob-release": "^2.1.2",
    "tcomb-forked": "^3.4.0"
  },
  "peerDependencies": {
    "liwi": ">=0.8.0 <1.0.0"
  }
}
