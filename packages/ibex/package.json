{
  "name": "ibex",
  "version": "7.0.0",
  "description": "Framework for browser, with the same API than koa 2",
  "keywords": [
    "alp"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/ibex"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/ibex/issues"
  },
  "browserslist": [
    "> 0.2%",
    "Firefox ESR",
    "not ie < 12",
    "not dead",
    "not op_mini all"
  ],
  "main": "./dist/index-browser.es.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "exports": {
    ".": {
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.mjs"
          },
          "import": "./dist/index-browsermodern.mjs"
        },
        "development": {
          "import": "./dist/index-browser-dev.mjs",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.mjs",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.mjs,.ts --report-unused-disable-directives --quiet packages/ibex",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/koa": "^2.11.6",
    "nightingale-logger": "^11.0.0",
    "query-string": "^6.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "26.3.0",
    "rollup": "2.36.2"
  }
}
