{
  "name": "ibex",
  "version": "6.0.0",
  "description": "Framework for browser, with the same API than koa 2",
  "keywords": ["alp"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/ibex"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/ibex/issues"
  },
  "browserslist": ["> 0.1%", "Firefox ESR", "not ie < 9", "not dead"],
  "main": "./dist/index-browser.es.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "eslint --ext .js,.ts --quiet src",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": ["es"]
      },
      {
        "target": "browser",
        "formats": ["cjs", "es"]
      }
    ],
    "entries": ["index"],
    "withReact": false
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@types/koa": "^2.11.0",
    "delegates": "^1.0.0",
    "nightingale-logger": "^6.2.2",
    "query-string": "^6.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@pob/eslint-config-typescript": "33.0.8",
    "babel-preset-modern-browsers": "14.1.1",
    "eslint": "6.7.2",
    "pob-babel": "24.1.0",
    "prettier": "1.19.1"
  }
}
