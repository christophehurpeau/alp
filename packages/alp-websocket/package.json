{
  "name": "alp-websocket",
  "version": "3.1.0",
  "description": "websocket in alp framework",
  "keywords": [
    "alp",
    "websocket",
    "socket.io",
    "socketio"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-websocket.git",
  "homepage": "https://github.com/alpjs/alp-websocket#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-websocket/issues"
  },
  "main": "index.js",
  "webpack:main": "./lib-webpack/browser.js",
  "webpack:main-dev": "./lib-webpack-dev/browser.js",
  "webpack:main-modern-browsers": "./lib-webpack-modern-browsers/browser.js",
  "webpack:main-modern-browsers-dev": "./lib-webpack-modern-browsers-dev/browser.js",
  "scripts": {
    "build": "pob-build",
    "build:dev": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx --fix src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch",
    "watch:dev": "pob-watch"
  },
  "dependencies": {
    "nightingale-logger": "^1.3.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-modern-browsers": "^5.1.0",
    "babel-preset-modern-browsers-stage-1": "^1.0.0",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb-base": "^7.1.0",
    "eslint-config-pob": "^7.3.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "pob-babel": "^8.0.3",
    "pob-release": "^2.1.2",
    "tcomb-forked": "^3.3.0"
  }
}
