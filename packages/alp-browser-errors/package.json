{
  "name": "alp-browser-errors",
  "version": "2.1.1",
  "description": "display errors in alp framework",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-browser-errors"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "browserslist": [
    "> 0.1%",
    "Firefox ESR",
    "not ie < 9",
    "not dead"
  ],
  "main": "./dist/index-browser.es.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "yarn --cwd ../.. lint packages/alp-browser-errors",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "alp-types": "^2.0.0",
    "error-html": "^0.3.5",
    "nightingale-logger": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "babel-preset-modern-browsers": "14.2.1",
    "pob-babel": "24.14.1"
  }
}
