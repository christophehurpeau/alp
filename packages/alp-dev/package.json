{
  "name": "alp-dev",
  "version": "8.0.0-beta4",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": [
    "alp",
    "framework",
    "webpack"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-dev.git",
  "homepage": "https://github.com/alpjs/alp-dev#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "lib/index.js",
  "bin": {
    "alp-dev": "./dev",
    "alp-dev-build": "./dev-build",
    "alp-dev-build-prod": "./dev-build-prod",
    "alp-dev-clean": "./dev-clean",
    "webpack": "./webpack"
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "prepare": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true ; ln -s ../../git-hooks/prepare-commit-msg .git/hooks/prepare-commit-msg 2>/dev/null || true",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true ; ln -s ../../git-hooks/prepare-commit-msg .git/hooks/prepare-commit-msg 2>/dev/null || true",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-plugin-discard-module-references": "^1.1.2",
    "babel-plugin-flow-runtime": "^0.10.0",
    "babel-plugin-minify-empty-function": "0.0.1",
    "babel-plugin-transform-remove-debugger": "^6.8.1",
    "babel-preset-babili-optimizations": "^0.2.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-modern-browsers": "^9.0.2",
    "babel-preset-pob": "^0.4.3",
    "babel-preset-pob-react": "^0.2.2",
    "babel-preset-pob-stages": "^0.1.2",
    "babel-regenerator-runtime": "^6.5.0",
    "babili-webpack-plugin": "^0.0.11",
    "banner-content-loader": "^0.1.1",
    "css-loader": "^0.28.0",
    "eslint-loader": "^1.7.1",
    "execa": "^0.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-runtime": "^0.10.0",
    "glob": "^7.1.1",
    "js-yaml": "^3.8.3",
    "minimist-argv": "^1.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.2",
    "offline-plugin": "^4.7.0",
    "pobpack-browser": "^0.5.0",
    "pobpack-node": "^0.7.0",
    "portscanner": "^2.1.1",
    "postcss": "^5.2.17",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "springbokjs-daemon": "^2.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "ynnub": "^0.2.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-latest-node": "^0.2.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-pob": "^11.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "pob-babel": "^17.2.0",
    "pob-release": "^3.1.0"
  }
}
