{
  "name": "alp-dev",
  "version": "15.3.1",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": [
    "alp",
    "framework",
    "webpack"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/alp-dev"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./watch": {
      "node": {
        "import": "./dist/watch-node14.mjs"
      }
    },
    "./watch-browser": {
      "node": {
        "import": "./dist/watch-browser-node14.mjs"
      }
    },
    "./watch-node": {
      "node": {
        "import": "./dist/watch-node-node14.mjs"
      }
    },
    "./build": {
      "node": {
        "import": "./dist/build-node14.mjs"
      }
    },
    "./build-modern-browser": {
      "node": {
        "import": "./dist/build-modern-browser-node14.mjs"
      }
    },
    "./build-node": {
      "node": {
        "import": "./dist/build-node-node14.mjs"
      }
    },
    "./build-older-browser": {
      "node": {
        "import": "./dist/build-older-browser-node14.mjs"
      }
    }
  },
  "module:node": "./dist/index-node14.mjs",
  "module:aliases-node": {
    "./watch.js": "./dist/watch-node14.es.js",
    "./watch-browser.js": "./dist/watch-browser-node14.es.js",
    "./watch-node.js": "./dist/watch-node-node14.es.js",
    "./build.js": "./dist/build-node14.es.js",
    "./build-modern-browser.js": "./dist/build-modern-browser-node14.es.js",
    "./build-node.js": "./dist/build-node-node14.es.js",
    "./build-older-browser.js": "./dist/build-older-browser-node14.es.js"
  },
  "sideEffects": false,
  "bin": {
    "alp-dev": "./bin/dev.js",
    "alp-dev-build": "./bin/dev-build.js",
    "alp-dev-build-prod": "./bin/dev-build-prod",
    "alp-dev-clean": "./bin/dev-clean",
    "webpack": "./bin/webpack.js"
  },
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/alp-dev",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "watch",
      "watch-browser",
      "watch-node",
      "build",
      "build-modern-browser",
      "build-node",
      "build-older-browser"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.16.5"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-discard-module-references": "^1.1.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-inline-classnames-babel7": "^1.1.9",
    "babel-preset-modern-browsers": "^15.0.2",
    "babel-preset-pob-env": "^7.1.4",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "colorette": "^2.0.16",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.0",
    "debounce": "^1.2.1",
    "execa": "^5.0.0",
    "glob": "^7.1.6",
    "ignore-loader": "^0.1.2",
    "js-yaml": "^4.1.0",
    "less-loader": "^5.0.0",
    "less-modify-var-loader": "^1.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "minimist-argv": "^1.1.0",
    "nightingale": "^12.1.2",
    "nightingale-ansi-formatter": "^12.1.2",
    "nightingale-console": "^12.1.2",
    "nightingale-logger": "^12.1.2",
    "offline-plugin": "^5.0.7",
    "portscanner": "^2.2.0",
    "postcss": "^7.0.23",
    "postcss-loader": "^6.2.1",
    "progress": "^2.0.3",
    "react-dev-utils": "12.0.0",
    "react-refresh": "^0.11.0",
    "sass": "^1.32.8",
    "sass-loader": "^12.4.0",
    "source-map-support": "^0.5.21",
    "springbokjs-daemon": "^8.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "typed-css-modules-loader": "^0.0.18",
    "webpack": "^5.65.0",
    "webpack-dev-server": "^4.6.0",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@types/autoprefixer": "9.7.2",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.6",
    "@types/css-minimizer-webpack-plugin": "3.2.1",
    "@types/debounce": "1.2.1",
    "@types/execa": "2.0.0",
    "@types/glob": "7.1.3",
    "@types/js-yaml": "4.0.5",
    "@types/mini-css-extract-plugin": "2.4.0",
    "@types/portscanner": "2.1.1",
    "@types/progress": "2.0.5",
    "@types/react-dev-utils": "9.0.9",
    "@types/terser-webpack-plugin": "5.2.0",
    "@types/webpack-dev-server": "4.5.0",
    "@types/webpack-node-externals": "2.5.3",
    "@types/workbox-webpack-plugin": "5.1.8",
    "less": "4.1.2",
    "pob-babel": "29.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.5.4"
  }
}
