{
  "name": "alp-dev",
  "version": "14.0.1",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": [
    "alp",
    "framework",
    "webpack"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-dev"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./watch.js": "./dist/watch-node10.es.js",
    "./watch-browser.js": "./dist/watch-browser-node10.es.js",
    "./watch-node.js": "./dist/watch-node-node10.es.js",
    "./build.js": "./dist/build-node10.es.js",
    "./build-modern-browser.js": "./dist/build-modern-browser-node10.es.js",
    "./build-node.js": "./dist/build-node-node10.es.js",
    "./build-older-browser.js": "./dist/build-older-browser-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./watch.js": "./dist/watch-node10-dev.es.js",
    "./watch-browser.js": "./dist/watch-browser-node10-dev.es.js",
    "./watch-node.js": "./dist/watch-node-node10-dev.es.js",
    "./build.js": "./dist/build-node10-dev.es.js",
    "./build-modern-browser.js": "./dist/build-modern-browser-node10-dev.es.js",
    "./build-node.js": "./dist/build-node-node10-dev.es.js",
    "./build-older-browser.js": "./dist/build-older-browser-node10-dev.es.js"
  },
  "bin": {
    "alp-dev": "./dev",
    "alp-dev-build": "./dev-build",
    "alp-dev-build-prod": "./dev-build-prod",
    "alp-dev-clean": "./dev-clean",
    "webpack": "./webpack"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "yarn --cwd ../.. lint packages/alp-dev",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "watch",
      "watch-browser",
      "watch-node",
      "build",
      "build-modern-browser",
      "build-node",
      "build-older-browser"
    ]
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^9.7.3",
    "babel-plugin-discard-module-references": "^1.1.2",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-classnames-babel7": "^1.1.9",
    "babel-preset-latest-node": "^4.0.1",
    "babel-preset-modern-browsers": "^14.1.1",
    "babel-preset-pob-env": "^3.7.1",
    "colorette": "^1.1.0",
    "console-loader": "^1.0.0",
    "css-loader": "^4.2.1",
    "execa": "^2.0.0",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "file-loader": "^4.0.0",
    "glob": "^7.1.6",
    "ignore-loader": "^0.1.2",
    "js-yaml": "^3.13.1",
    "less-loader": "^5.0.0",
    "less-modify-var-loader": "^1.0.0",
    "minimist-argv": "^1.1.0",
    "mkdirp": "^0.5.1",
    "nightingale": "^10.0.1",
    "nightingale-ansi-formatter": "^10.0.1",
    "nightingale-console": "^10.0.1",
    "node-sass": "^4.13.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pob-babel": "^24.12.0",
    "pobpack-browser": "9.0.0",
    "pobpack-node": "9.0.0",
    "pobpack-types": "4.1.2",
    "portscanner": "^2.2.0",
    "postcss": "^7.0.23",
    "postcss-loader": "^3.0.0",
    "progress": "^2.0.3",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "springbokjs-daemon": "^5.1.0",
    "typed-css-modules-loader": "^0.0.18",
    "url-loader": "^3.0.0",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@types/autoprefixer": "9.6.1",
    "@types/execa": "2.0.0",
    "@types/glob": "7.1.3",
    "@types/js-yaml": "3.12.5",
    "@types/mkdirp": "0.5.2",
    "@types/optimize-css-assets-webpack-plugin": "5.0.1",
    "@types/portscanner": "2.1.0",
    "@types/progress": "2.0.3",
    "@types/webpack-dev-server": "3.11.1",
    "less": "3.10.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
