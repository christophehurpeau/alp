{
  "name": "alp-dev",
  "version": "2.19.0",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": [
    "alp",
    "framework",
    "webpack"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "alpjs/alp-dev",
  "homepage": "https://github.com/alpjs/alp-dev#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "main": "lib/index.js",
  "bin": {
    "alp-dev": "./dev",
    "alp-dev-build": "./dev-build",
    "alp-dev-build-config": "./dev-build"
  },
  "scripts": {
    "build": "pob-build",
    "build:dev": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx --fix -c .eslintrc.yml src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit || echo",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch",
    "watch:dev": "pob-watch"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-modern-browsers": "^6.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-regenerator-runtime": "^6.5.0",
    "browser-sync": "^2.17.2",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "gulp": "gulpjs/gulp#70cde1ee19e4425a2cdc5972e9f820677ece3a0a",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.5.0",
    "gulp-util": "^3.0.7",
    "js-yaml": "^3.6.1",
    "minimist-argv": "^1.0.0",
    "offline-plugin": "^3.4.2",
    "pob-babel": "^11.0.0",
    "portscanner": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "source-map-loader": "^0.1.5",
    "springbokjs-daemon": "^1.3.0",
    "stylus": "^0.54.5",
    "through2": "^2.0.1",
    "webpack": "christophehurpeau/webpack#9d9e3fa0cf43c2057df1e4a9e968099bc472fad4",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-1": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-config-pob": "^7.6.0",
    "eslint-plugin-import": "^2.0.0",
    "pob-release": "^2.2.2",
    "tcomb-forked": "^3.4.0"
  }
}
