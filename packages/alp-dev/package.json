{
  "name": "alp-dev",
  "version": "10.2.1",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": [
    "alp",
    "framework",
    "webpack"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-dev.git",
  "homepage": "https://github.com/alpjs/alp-dev#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "lib/index.js",
  "bin": {
    "alp-dev": "./dev",
    "alp-dev-build": "./dev-build",
    "alp-dev-build-prod": "./dev-build-prod",
    "alp-dev-clean": "./dev-clean",
    "webpack": "./webpack"
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-plugin-discard-module-references": "^1.1.2",
    "babel-plugin-flow-runtime": "0.10.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest-node": "^0.4.0",
    "babel-preset-modern-browsers": "^10.0.0",
    "babel-preset-optimizations": "^1.2.1",
    "babel-preset-pob": "^0.5.0",
    "babel-preset-pob-react": "^0.2.4",
    "babel-preset-pob-stages": "^0.2.4",
    "babel-regenerator-runtime": "^6.5.0",
    "babili-webpack-plugin": "^0.1.2",
    "banner-content-loader": "^0.1.1",
    "cache-loader": "^1.0.3",
    "console-loader": "^1.0.0",
    "css-loader": "^0.28.5",
    "eslint-loader": "^1.9.0",
    "execa": "^0.8.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-runtime": "0.10.0",
    "glob": "^7.1.2",
    "js-yaml": "^3.9.1",
    "minimist-argv": "^1.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "offline-plugin": "^4.8.3",
    "pobpack-browser": "^2.0.2",
    "pobpack-node": "^2.0.2",
    "portscanner": "^2.1.1",
    "postcss": "^6.0.9",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "springbokjs-daemon": "^2.1.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-pob": "^16.0.4",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.4",
    "pob-babel": "^18.1.5",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3",
    "react": "^16.0.0-beta.5",
    "react-dom": "^16.0.0-beta.5",
    "ynnub": "^0.3.0"
  },
  "peerDependencies": {
    "ynnub": "^0.3.0"
  }
}
