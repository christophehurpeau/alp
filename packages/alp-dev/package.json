{
  "name": "alp-dev",
  "version": "5.0.3",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": [
    "alp",
    "framework",
    "webpack"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-dev.git",
  "homepage": "https://github.com/alpjs/alp-dev#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "main": "lib/index.js",
  "webpack:node": "./lib-node6/index.js",
  "webpack:node-dev": "./lib-node6-dev/index.js",
  "bin": {
    "alp-dev": "./dev",
    "alp-dev-build": "./dev-build",
    "alp-dev-build-config": "./dev-build"
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .js,.jsx src/",
    "prepare": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true ; ln -s ../../git-hooks/prepare-commit-msg .git/hooks/prepare-commit-msg 2>/dev/null || true",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true ; ln -s ../../git-hooks/prepare-commit-msg .git/hooks/prepare-commit-msg 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-minify-empty-function": "0.0.1",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-preset-babili": "^0.0.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-modern-browsers": "^7.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-regenerator-runtime": "^6.5.0",
    "browser-sync": "^2.18.1",
    "css-loader": "^0.26.1",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "glob": "^7.1.1",
    "js-yaml": "^3.7.0",
    "minimist-argv": "^1.0.0",
    "offline-plugin": "^4.5.3",
    "pob-babel": "^12.5.0",
    "portscanner": "^2.1.1",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.1",
    "postcss-modules": "^0.5.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "source-map-loader": "^0.1.5",
    "springbokjs-daemon": "^1.4.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-config-pob": "^10.0.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "komet": "^0.1.3",
    "komet-karma": "^0.2.3",
    "pob-babel": "^12.5.0",
    "pob-release": "^2.3.0",
    "tcomb-forked": "^3.4.0"
  }
}
