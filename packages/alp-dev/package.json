{
  "name": "alp-dev",
  "version": "10.6.3",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": ["alp", "framework", "webpack"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:alpjs/alp-dev.git",
  "homepage": "https://github.com/alpjs/alp-dev#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "lib/index.js",
  "bin": {
    "alp-dev": "./dev",
    "alp-dev-build": "./dev-build",
    "alp-dev-build-prod": "./dev-build-prod",
    "alp-dev-clean": "./dev-clean",
    "webpack": "./webpack"
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint src/",
    "postcheckout": "yarnhook",
    "postmerge": "yarnhook",
    "postrewrite": "yarnhook",
    "precommit": "lint-staged",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": ["prettier --write", "git add"],
    "src/**/*.json": ["prettier --write", "git add"],
    "src/**/*.js": ["eslint --fix --quiet", "git add"]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "dependencies": {
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-flow": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "autoprefixer": "^8.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-discard-module-references": "^1.1.2",
    "babel-preset-latest-node": "^2.0.0-beta.2",
    "babel-preset-modern-browsers": "^12.0.0-beta.0",
    "babel-preset-pob-env": "^2.2.2",
    "console-loader": "^1.0.0",
    "css-loader": "^0.28.11",
    "execa": "^0.10.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "js-yaml": "^3.11.0",
    "mini-css-extract-plugin": "^0.4.0",
    "minimist-argv": "^1.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "offline-plugin": "^5.0.2",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "pobpack-browser": "^5.0.6",
    "pobpack-node": "^5.0.6",
    "portscanner": "^2.1.1",
    "postcss": "^6.0.22",
    "postcss-loader": "^2.1.4",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "springbokjs-daemon": "^2.7.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.18.2",
    "eslint-config-pob": "^19.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "pob-babel": "^22.2.4",
    "pob-release": "^4.1.1",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "yarnhook": "^0.2.0",
    "ynnub": "^0.6.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "ynnub": ">=0.6.0"
  }
}
