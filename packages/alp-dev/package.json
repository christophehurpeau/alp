{
  "name": "alp-dev",
  "version": "12.6.1",
  "description": "Config for webpack/browsersync/makefile for alp server",
  "keywords": ["alp", "framework", "webpack"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/alp.git",
    "directory": "packages/alp-dev"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "bugs": {
    "url": "https://github.com/alpjs/alp-dev/issues"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./watch.js": "./dist/watch-node10.es.js",
    "./watch-browser.js": "./dist/watch-browser-node10.es.js",
    "./watch-node.js": "./dist/watch-node-node10.es.js",
    "./build.js": "./dist/build-node10.es.js",
    "./build-modern-browser.js": "./dist/build-modern-browser-node10.es.js",
    "./build-node.js": "./dist/build-node-node10.es.js",
    "./build-older-browser.js": "./dist/build-older-browser-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./watch.js": "./dist/watch-node10-dev.es.js",
    "./watch-browser.js": "./dist/watch-browser-node10-dev.es.js",
    "./watch-node.js": "./dist/watch-node-node10-dev.es.js",
    "./build.js": "./dist/build-node10-dev.es.js",
    "./build-modern-browser.js": "./dist/build-modern-browser-node10-dev.es.js",
    "./build-node.js": "./dist/build-node-node10-dev.es.js",
    "./build-older-browser.js": "./dist/build-older-browser-node10-dev.es.js"
  },
  "bin": {
    "alp-dev": "./dev",
    "alp-dev-build": "./dev-build",
    "alp-dev-build-prod": "./dev-build-prod",
    "alp-dev-clean": "./dev-clean",
    "webpack": "./webpack"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": [
      "watch",
      "watch-browser",
      "watch-node",
      "build",
      "build-modern-browser",
      "build-node",
      "build-older-browser"
    ],
    "withReact": false
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-plugin-discard-module-references": "^1.1.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-inline-classnames-babel7": "^1.1.9",
    "babel-preset-latest-node": "^3.0.1",
    "babel-preset-modern-browsers": "^14.0.0",
    "babel-preset-pob-env": "^2.24.0",
    "colorette": "^1.0.7",
    "console-loader": "^1.0.0",
    "css-loader": "^2.1.1",
    "execa": "^1.0.0",
    "extracted-loader": "^1.0.7",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "ignore-loader": "^0.1.2",
    "js-yaml": "^3.13.1",
    "less-loader": "^5.0.0",
    "less-modify-var-loader": "^1.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "minimist-argv": "^1.1.0",
    "mkdirp": "^0.5.1",
    "nightingale": "^9.2.3",
    "nightingale-ansi-formatter": "^4.2.2",
    "nightingale-console": "^6.1.0",
    "node-sass": "^4.11.0",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pob-babel": "^23.20.0",
    "pobpack-browser": "^7.7.4",
    "pobpack-node": "^7.7.1",
    "pobpack-types": "^3.5.0",
    "portscanner": "^2.2.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "progress": "^2.0.3",
    "react-hot-loader": "^4.8.4",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "springbokjs-daemon": "^3.6.0",
    "typed-css-modules-loader": "^0.0.17",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-dev-server": "^3.3.1",
    "ynnub-webpack-config": "2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@pob/eslint-config-babel-node": "32.0.7",
    "@pob/eslint-config-typescript": "32.0.8",
    "@types/autoprefixer": "^9.4.0",
    "@types/execa": "^0.9.0",
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^3.12.1",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/portscanner": "^2.1.0",
    "@types/progress": "^2.0.3",
    "@types/webpack-dev-server": "^3.1.5",
    "eslint": "6.3.0",
    "less": "^3.9.0",
    "prettier": "1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
