{
  "name": "react-alp-route-match-transition",
  "version": "2.2.1",
  "description": "route match with transition",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alp.git",
    "directory": "packages/react-alp-route-match-transition"
  },
  "homepage": "https://github.com/christophehurpeau/alp",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "main": "./dist/index-node14.mjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node14.mjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js"
      }
    }
  },
  "module:node": "./dist/index-node14.mjs",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/react-alp-route-match-transition",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "14",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ],
    "jsx": true
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-router": "^5.0.0",
    "react-transition-group": "^4.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "type-fest": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "@types/react-router": "5.1.17",
    "@types/react-transition-group": "4.4.1",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "29.6.0",
    "react": "17.0.2",
    "react-router": "5.2.0",
    "react-transition-group": "4.4.1",
    "typescript": "4.5.4"
  }
}
